package maks;
import java.util.*
 
public class Maks {

    
    public static void main(String[] args) {
        Scanner Baca = new Scanner(System.in);
        int N,B1,B=-999;
        System.out.print("Banyaknya data = "); N= Baca.nextInt();
        
        for(int i=1;i<=N;i++){
         System.out.print("Data ke - "+i+"="); B1= Baca.nextInt();  
         if (B<B1) B=B1;
        }
        
              
        System.out.println("Bilangan terbesar = "+B);   
    }
    
}
