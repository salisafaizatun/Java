package loop3;



/**
 * file:loop3.java
 * loop bersarang
 */
public class Loop3 {

    
    public static void main(String[] args) {
        int i,j,n=2,m=3;  
        
        System.out.println("for:");
        
        for(i=1;i<=n;i++) {
           for(j=1;j<=m;j++) {
            System.out.println(i+" + "+j+" = "+(i+j));
           }
           System.out.println(" ");
        } 
        
        System.out.println("\nwhile:");
        i=1;
        while (i<=n){
            j=1;
            while(j<=m){
              System.out.println(i+" + "+j+" = "+(i+j));
              j++;
            }
             System.out.println(" ");
            i++;
        } 
        
        
        System.out.println("\ndo while:");
         i=1;
        do {
            j=1;
            do {
            System.out.println(i+" + "+j+" = "+(i+j));
             j++;
            } while(j<=m) ;
             System.out.println(" ");
            i++;
        } while(i<=n) ;    
    
    }
    
}
=======================================<< Output >>==============================================
for:
1 + 1 =2
1 + 2 = 3
1 + 3 = 4

2 + 1 = 3
2 + 2 = 4
2 + 3 = 5

while:
1 + 1 =2
1 + 2 = 3
1 + 3 = 4

2 + 1 = 3
2 + 2 = 4
2 + 3 = 5

do while:
1 + 1 =2
1 + 2 = 3
1 + 3 = 4

2 + 1 = 3
2 + 2 = 4
2 + 3 = 5

BUILD SUCCESSFUL (total time: 0 seconds)
