package loop4a;

import java.util.Scanner;


public class Loop4a {

   
    public static void main(String[] args) {
      int n;  
	System.out.println("Data masukan tidak boleh urut")
        Scanner Baca = new Scanner(System.in);
        System.out.print("Banyaknya data= ");n=Baca.nextInt();
        if (n>0){
            
             int i=1,j=0,k=0;
             float x,max=-9999,min=9999,total=0;
            while(i<=n) {
             	System.out.print("data ke "+i+" = ");
             	x=Baca.nextInt();
             	total +=x;
             	if(max<x) {max=x;j++;}
                 else if (min>x){ min=x;k++;}  
             	i++;
         
	     }
 	if(i*j !=0){
       		 float range=max-min;
	         float rata2=total/n;
	         System.out.println(" bilangan terbesar ="+max);
        	 System.out.println(" bilangan terkecil ="+min);
	         System.out.println(" Range             ="+range);
        	 System.out.println(" Total             ="+total);
         	 System.out.println(" rata-rata         ="+rata2);
         }
	else System.out.println(" kondisi data terurut);
        }
    }
    
}
