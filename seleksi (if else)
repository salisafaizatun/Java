import java.util.Scanner;
import static java.lang.System.in;
import static java.lang.System.out;

/**
 * File = seleksi.java
 * cetak nilai fungsi f(x) = 0 ; untuk x<0 
 * cetak nilai fungsi f(x) = 5 ; untuk 0<=x<10
 * cetak nilai fungsi f(x) = 3 ; untuk 10<=x<25
 * cetak nilai fungsi f(x) = 6 ; untuk 25<=x<50
 * cetak nilai fungsi f(x) = 7 ; untuk x>=50 
 */
public class Seleksi {

    public static void main(String[] args) {
      
        Scanner Baca = new Scanner(System.in);
        int x,f;
        out.print("x = "); x= Baca.nextInt();
        if (x<0) f=0;
        else if ((x>=0)&& (x<10)) f=5;
        else if ((x>=10)&& (x<25)) f=3;
        else if ((x>=25)&& (x<50)) f=6;
        else  f=7;  // x>=50

        out.println("f("+x+") = "+f) 
    }
  
    
}
===============================<<    Output     >>=================================
run:
x = 9
f(9) = 5
BUILD SUCCESSFUL (total time: 3 seconds)

