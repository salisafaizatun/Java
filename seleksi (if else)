import java.util.Scanner;
import static java.lang.System.in;
import static java.lang.System.out;

/**
 * File = seleksi.java
 * cetak nilai fungsi f(x) = 5 ; untuk 0<=x<=20
 * cetak nilai fungsi f(x) = 0 ; untuk x<0 atau x>20   
 */
public class Seleksi {

    public static void main(String[] args) {
      
        Scanner Baca = new Scanner(System.in);
        int x,f;
        out.print("x = "); x= Baca.nextInt();
        if ((x<0) ||(x>20)) f=0;
        else f=5;       // 0<=x<=20
    
        out.println("f("+x+") = "+f) 
    }
    
}
===============================<<    Output     >>=================================
run:
x = 7
f(7) = 5
BUILD SUCCESSFUL (total time: 6 seconds)

===============================<< Batas Program >>=================================


import java.util.Scanner;
import static java.lang.System.in;
import static java.lang.System.out;

/**
 * File = seleksi.java
 * cetak nilai fungsi f(x) = 5 ; untuk 0<=x<=20
 * cetak nilai fungsi f(x) = 0 ; untuk x<0 atau x>20   
 */
public class Seleksi {

    public static void main(String[] args) {
      
        Scanner Baca = new Scanner(System.in);
        int x,f;
        out.print("x = "); x= Baca.nextInt();
        if ((x>=0) && (x<=20)) f=5;
        else f=0;       // x<0 atau x>20
    
        out.println("f("+x+") = "+f) 
    }
    
}
===============================<<    Output     >>=================================
