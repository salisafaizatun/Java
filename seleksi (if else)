import java.util.Scanner;
import static java.lang.System.in;
import static java.lang.System.out;

/**
 * file =seleksi.java
 * cari akar akar persamaan kuadrat Ax^2+Bx+c=0 dan cetak hasilnya;
 */
public class Seleksi {

    
    public static void main(String[] args) {
    
        Scanner Baca = new Scanner(System.in);  
        double A,B,C;
        
        out.print("A = "); A= Baca.nextInt();
        out.print("B = "); B= Baca.nextInt();
        out.print("C = "); C= Baca.nextInt();
        
        if(A==0)
            if(B==0)
                if(C==0) out.println("Solusi banyaknya tak berhingga");
                else out.println("tidak memiliki jawaban");
            else {
                  double x= -C/B;
                  out.println("X = "+x);
            }
        else { //  A <>0
            double D=B*B-4*A*C;
            if(D<0){
                double xr =-B/(2*A); 
                double xim = Math.sqrt(-D)/2/A;
                out.println("X1 = "+xr+" + "+xim+"i");
                out.println("X2 = "+xr+" - "+xim+"i");
            }
            else if(D==0){
                double x=-B/2/A;
                out.println("X1=X2= "+x);
            } 
            else { //D>0
                double x1 = (-B+Math.sqrt(D))/2/A;
                double x2 = (-B-Math.sqrt(D))/2/A;
                out.println("X1 = "+x1);
                out.println("X2 = "+x2);
                
            }
            
            }
        
    }
    
}

===============================<< Batas Program >>============================
run:
A = 5
B = 7
C = 9
X1 = -0.7 + 1.1445523142259597i
X2 = -0.7 - 1.1445523142259597i
BUILD SUCCESSFUL (total time: 7 seconds)

===============================<< Batas Program >>============================


import java.util.Scanner;
import static java.lang.System.in;
import static java.lang.System.out;

/**
 * File = seleksi.java
 * cetak nilai fungsi f(x) = 0 ; untuk x<0 
 * cetak nilai fungsi f(x) = 5 ; untuk 0<=x<10
 * cetak nilai fungsi f(x) = 3 ; untuk 10<=x<25
 * cetak nilai fungsi f(x) = 6 ; untuk 25<=x<50
 * cetak nilai fungsi f(x) = 7 ; untuk x>=50 
 */
public class Seleksi {

    public static void main(String[] args) {
      
        Scanner Baca = new Scanner(System.in);
        int x,f;
        out.print("x = "); x= Baca.nextInt();
        if (x<0) f=0;
        else if (x<10) f=5; // 0<=x<10
        else if (x<25) f=3; // 10<=x<25
        else if (x<50) f=6;  // 25<=x<50
        else  f=7;  // x>=50

        out.println("f("+x+") = "+f) 
    }
  
    
}
===============================<<    Output     >>=================================
run:
x = 5
f(5) = 5
BUILD SUCCESSFUL (total time: 3 seconds)

===============================<< Batas Program >>=================================

