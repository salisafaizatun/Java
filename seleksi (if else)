import static java.lang.System.out;
/**
 *File = seleksi.java
 *operator ralsional dan operator logika
 */
public class Seleksi {

    public static void main(String[] args) {
        int intA=10 , intB=20;
         out.println("Operator Relasional ");
         out.println("===================\n");
         out.println("intA = 10 dan  intB = 20");
         out.println("1. intA == intB bernilai "+ (intA == intB));
         out.println("2. intA != intB bernilai "+ (intA != intB));
         out.println("3. intA > intB bernilai "+ (intA > intB));
         out.println("4. intA < intB bernilai "+ (intA < intB));
         out.println("5. intA >= intB bernilai "+ (intA >= intB));
         out.println("6. intA <= intB bernilai "+ (intA <= intB));
         
         out.println("\n\nOperator Logika ");
         out.println("===================\n");
         out.println("1. true || true bernilai "+ (true || true));
         out.println("2. true || false bernilai "+ (true || false));
         out.println("3. false || true bernilai "+ (false || true));
         out.println("4. false || false bernilai "+ (false || false));
         out.println("5. true && true bernilai "+ (true && true));
         out.println("6. true && false bernilai "+ (true && false));
         out.println("7. false && true bernilai "+ (false && true));
         out.println("8. false && false bernilai "+ (false && false));
         out.println("9. !true bernilai "+ (!true));
         out.println("10.!false  bernilai "+ (!false));
    } 
}
===============================<<    Output     >>=================================
run:
Operator Relasional 
===================

intA = 10 dan  intB = 20
1. intA == intB bernilai false
2. intA != intB bernilai true
3. intA > intB bernilai false
4. intA < intB bernilai true
5. intA >= intB bernilai false
6. intA <= intB bernilai true


Operator Logika 
===================

1. true || true bernilai true
2. true || false bernilai true
3. false || true bernilai true
4. false || false bernilai false
5. true && true bernilai true
6. true && false bernilai false
7. false && true bernilai false
8. false && false bernilai false
9. !true bernilai false
10.!false  bernilai true
BUILD SUCCESSFUL (total time: 0 seconds)
===============================<< Batas Program >>=================================


import static java.lang.System.out;

/**
 * File = seleksi.java
 * komplemen
 */
public class Seleksi {

  
    public static void main(String[] args) {
        int intA=10 , intB=20;
         out.println("Operator Relasional ");
         out.println("===================\n");
         out.println("intA = 10 dan  intB = 20");
         out.print("1. !(intA == intB) bernilai "+ (!(intA == intB))); 
         out.print(" ekuivalen ");
         out.println(" intA != intB bernilai "+ (intA != intB));
         out.print("2. !(intA != intB) bernilai "+ (!(intA != intB))); 
         out.print(" ekuivalen ");
         out.println(" intA == intB bernilai "+ (intA == intB));
         out.print("3. !(intA > intB) bernilai "+ (!(intA > intB)));
         out.print(" ekuivalen ");
         out.println(" intA <= intB bernilai "+ (intA <= intB));
         out.print("4. !(intA <= intB) bernilai "+ (!(intA <= intB)));
         out.print(" ekuivalen ");
         out.print(" intA > intB bernilai "+ (intA > intB));
         
         out.println("\n\nOperator Logika ");
         out.println("===================\n");
         out.print("1. !(true || false) bernilai "+ (!(true || false)));
         out.print(" ekuivalen ");
         out.println(" false && true bernilai "+ (false && true));
         out.print("2. !(true && false) bernilai "+ (!(true && false)));
         out.print(" ekuivalen ");
         out.println(" false || true bernilai "+ (false || true));
         
                  
    }
    
}
===============================<<    Output     >>=================================
run:
Operator Relasional 
===================

intA = 10 dan  intB = 20
1. !(intA == intB) bernilai true ekuivalen  intA != intB bernilai true
2. !(intA != intB) bernilai false ekuivalen  intA == intB bernilai false
3. !(intA > intB) bernilai true ekuivalen  intA <= intB bernilai true
4. !(intA <= intB) bernilai false ekuivalen  intA > intB bernilai false

Operator Logika 
===================

1. !(true || false) bernilai false ekuivalen  false && true bernilai false
2. !(true && false) bernilai true ekuivalen  false || true bernilai true
BUILD SUCCESSFUL (total time: 1 second)
===============================<< Batas Program >>=================================


import static java.lang.System.out;

/**
 * File = seleksi.java
 * komplemen
 */
public class Seleksi {

  
    public static void main(String[] args) {
        int intA=10 , intB=20;
         out.println("Operator Relasional ");
         out.println("===================\n");
         out.println("intA = 10 dan  intB = 20");
         out.print("1. !(intA == intB) bernilai "+ (!(intA == intB))); 
         out.print(" ekuivalen ");
         out.println(" intA != intB bernilai "+ (intA != intB));
         out.print("2. !(intA != intB) bernilai "+ (!(intA != intB))); 
         out.print(" ekuivalen ");
         out.println(" intA == intB bernilai "+ (intA == intB));
         out.print("3. !(intA > intB) bernilai "+ (!(intA > intB)));
         out.print(" ekuivalen ");
         out.println(" intA <= intB bernilai "+ (intA <= intB));
         out.print("4. !(intA <= intB) bernilai "+ (!(intA <= intB)));
         out.print(" ekuivalen ");
         out.print(" intA > intB bernilai "+ (intA > intB));
         
         out.println("\n\nOperator Logika ");
         out.println("===================\n");
         out.print("1. !(true || false) bernilai "+ (!(true || false)));
         out.print(" ekuivalen ");
         out.println(" false && true bernilai "+ (false && true));
         out.print("2. !(true && false) bernilai "+ (!(true && false)));
         out.print(" ekuivalen ");
         out.println(" false || true bernilai "+ (false || true));
         
         out.print("3. !(intA<intB || intA==intB) bernilai "+ (!((intA<intB)||(intA==intB)))); 
         out.print(" ekuivalen ");
         out.println(" intA>=intB && intA != intB) bernilai "+ ((intA>=intB)&&(intA != intB)));
    }
    
}
===============================<<    Output     >>=================================
run:
Operator Relasional 
===================

intA = 10 dan  intB = 20
1. !(intA == intB) bernilai true ekuivalen  intA != intB bernilai true
2. !(intA != intB) bernilai false ekuivalen  intA == intB bernilai false
3. !(intA > intB) bernilai true ekuivalen  intA <= intB bernilai true
4. !(intA <= intB) bernilai false ekuivalen  intA > intB bernilai false

Operator Logika 
===================

1. !(true || false) bernilai false ekuivalen  false && true bernilai false
2. !(true && false) bernilai true ekuivalen  false || true bernilai true
3. !(intA<intB || intA==intB) bernilai false ekuivalen  intA>=intB && intA != intB) bernilai false
BUILD SUCCESSFUL (total time: 0 seconds)
