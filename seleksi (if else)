import java.util.Scanner;
import static java.lang.System.in;
import static java.lang.System.out;

/**
 * File : Seleksi
 * mencetak bilangan terbesar dari 2 buah bilangan
 */
public class Seleksi {

    public static void main(String[] args) {
         Scanner Baca = new Scanner(in);
        int B1,B2;
        out.print("Bilangan pertama = "); B1= Baca.nextInt();
        out.print("Bilangan kedua   = "); B2= Baca.nextInt();
        out.println("\nBilangan terbesar dari "+B1+" dan "+B2+" adalah "+(B1>B2? B1:B2));
        
    }
    
}
===============================<<    Output     >>=================================
run:
Bilangan pertama = 3
Bilangan kedua   = 4

Bilangan terbesar dari 3 dan 4 adalah 4
BUILD SUCCESSFUL (total time: 3 seconds)
