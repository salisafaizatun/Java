import static java.lang.System.out;
import java.util.Scanner;

/**
 *File = konversidetik.java
 * untuk menampilkan jumlah hari , jam, menit  dan detik darimasukan lamanya waktu dalam detik
 */
public class KonversiDetik {

   
    public static void main(String[] args) {
    int d,H,J,M,D;
    Scanner Baca = new Scanner(System.in);
    out.print("Masukkan jumlah detik = ");d=Baca.nextInt();
    D=d;
    H=D/86400; D=D%86400;
    J=D/3600; D=D%3600;
    M=D/60; D=D%60;
    out.print(d+" detik = ");
    out.print(H+" hari + ");
    out.print(J+" jam + ");
    out.print(M+" menit + ");
    out.println(D+" detik.");
    }
    
}
=================================<<   Output     >>========================
Masukkan jumlah detik = 35
35 detik = 0 hari + 0 jam + 0 menit +35 detik
BUILD SUCCESSFUL (total time : 0 seconds)
=================================<< Batas Program >>=======================
